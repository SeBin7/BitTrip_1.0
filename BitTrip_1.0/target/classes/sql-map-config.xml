<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	
	<!-- Alias 설정  -->
	<typeAliases>
		<typeAlias alias="user" type="com.bittrip.pro.user.BitUserVO" />
		<typeAlias alias="post" type="com.bittrip.pro.post.BitPostVO" />
	</typeAliases>

	<!-- Sql Mapper 설정 -->
	<mappers>
		<mapper resource="mappings/user-mapping.xml" />
		<mapper resource="mappings/post-mapping.xml" />
	</mappers>
</configuration>
